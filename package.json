{
    "name": "proxy-switch-craft",
    "displayName": "Proxy Switch Craft",
    "version": "1.0.0",
    "description": "Proxy Switch Craft is a simple and user-friendly browser extension that helps users easily manage and switch proxy settings.",
    "author": "biscuit_zhou@outlook.com",
    "scripts": {
        "dev": "plasmo dev",
        "build": "plasmo build",
        "package": "plasmo package"
    },
    "dependencies": {
        "@emotion/react": "11.11.0",
        "@emotion/styled": "11.11.0",
        "@mui/icons-material": "5.15.0",
        "@mui/material": "5.15.0",
        "@plasmohq/storage": "^1.15.0",
        "plasmo": "0.90.5",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "4.1.1",
        "@types/chrome": "0.0.258",
        "@types/node": "20.11.5",
        "@types/react": "18.2.48",
        "@types/react-dom": "18.2.18",
        "prettier": "3.2.4",
        "typescript": "5.3.3"
    },
    "manifest": {
        "host_permissions": [
            "http://*/*",
            "https://*/*"
        ],
        "permissions": [
            "storage",
            "webRequest",
            "proxy"
        ],
        "incognito": "split",
        "default_locale": "en"
    }
}
